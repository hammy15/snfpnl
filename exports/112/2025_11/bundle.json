  "glossary": [
    {
      "term": "Per Patient Day",
      "abbreviation": "PPD",
      "definition": "Metric calculated using total resident/patient days as the denominator. Includes all payers.",
      "denominator_type": "resident_days"
    },
    {
      "term": "Per Skilled Day",
      "abbreviation": "PSD",
      "definition": "Metric calculated using skilled days as the denominator. Skilled days = Medicare A + Medicare Advantage (HMO) + Commercial + VA + ISNP days.",
      "denominator_type": "skilled_days"
    },
    {
      "term": "Per Vent Day",
      "abbreviation": "PVD",
      "definition": "Metric calculated using ventilator patient days as the denominator.",
      "denominator_type": "vent_days"
    },
    {
      "term": "Skilled Days",
      "abbreviation": "SD",
      "definition": "Total days for patients covered by skilled payers: Medicare Part A, Medicare Advantage (MA/HMO), Commercial skilled, VA skilled, and ISNP.",
      "denominator_type": "skilled_days"
    },
    {
      "term": "Resident Days",
      "abbreviation": "RD",
      "definition": "Total patient days across all payer types for the period.",
      "denominator_type": "resident_days"
    },
    {
      "term": "Skilled Mix",
      "abbreviation": "SM%",
      "definition": "Percentage of total resident days that are skilled days. Formula: (Skilled Days / Resident Days) × 100.",
      "denominator_type": "skilled_days",
      "payer_scope": "skilled"
    },
    {
      "term": "Total Revenue PPD",
      "abbreviation": "snf_total_revenue_ppd",
      "definition": "Total revenue per patient day across all payers. Formula: Total Revenue / Resident Days",
      "denominator_type": "resident_days",
      "payer_scope": "all"
    },
    {
      "term": "Skilled Revenue PSD",
      "abbreviation": "snf_skilled_revenue_psd",
      "definition": "Revenue from skilled payers per skilled day. Skilled = Medicare A + MA + Commercial + VA + ISNP.. Formula: Skilled Revenue / Skilled Days",
      "denominator_type": "skilled_days",
      "payer_scope": "skilled"
    },
    {
      "term": "Medicare A Revenue PSD",
      "abbreviation": "snf_medicare_a_revenue_psd",
      "definition": "Medicare Part A revenue per Medicare A day. Formula: Medicare A Revenue / Medicare A Days",
      "denominator_type": "payer_days",
      "payer_scope": "MEDICARE_A"
    },
    {
      "term": "Medicare Advantage Revenue PSD",
      "abbreviation": "snf_ma_revenue_psd",
      "definition": "Medicare Advantage (HMO) revenue per MA day. Formula: MA Revenue / MA Days",
      "denominator_type": "payer_days",
      "payer_scope": "MEDICARE_ADVANTAGE"
    },
    {
      "term": "Medicaid Revenue PPD",
      "abbreviation": "snf_medicaid_revenue_ppd",
      "definition": "Medicaid revenue per Medicaid day. Formula: Medicaid Revenue / Medicaid Days",
      "denominator_type": "payer_days",
      "payer_scope": "MEDICAID"
    },
    {
      "term": "Skilled Mix %",
      "abbreviation": "snf_skilled_mix_pct",
      "definition": "Percentage of total patient days that are skilled days. Formula: (Skilled Days / Resident Days) × 100",
      "denominator_type": "resident_days",
      "payer_scope": "skilled"
    },
    {
      "term": "Medicare A Mix %",
      "abbreviation": "snf_medicare_a_mix_pct",
      "definition": "Percentage of total patient days that are Medicare A days. Formula: (Medicare A Days / Resident Days) × 100",
      "denominator_type": "resident_days",
      "payer_scope": "MEDICARE_A"
    },
    {
      "term": "Medicare Advantage Mix %",
      "abbreviation": "snf_ma_mix_pct",
      "definition": "Percentage of total patient days that are MA/HMO days. Formula: (MA Days / Resident Days) × 100",
      "denominator_type": "resident_days",
      "payer_scope": "MEDICARE_ADVANTAGE"
    },
    {
      "term": "Total Operating Cost PPD",
      "abbreviation": "snf_total_cost_ppd",
      "definition": "Total operating expenses per patient day. Formula: Total Operating Expenses / Resident Days",
      "denominator_type": "resident_days",
      "payer_scope": "all"
    },
    {
      "term": "Nursing Cost PPD",
      "abbreviation": "snf_nursing_cost_ppd",
      "definition": "Total nursing department expenses per patient day. Formula: Total Nursing Expenses / Resident Days",
      "denominator_type": "resident_days",
      "payer_scope": "all"
    },
    {
      "term": "Therapy Cost PSD",
      "abbreviation": "snf_therapy_cost_psd",
      "definition": "Therapy expenses per skilled day. Formula: Total Therapy Expenses / Skilled Days",
      "denominator_type": "skilled_days",
      "payer_scope": "skilled"
    },
    {
      "term": "Ancillary Cost PSD",
      "abbreviation": "snf_ancillary_cost_psd",
      "definition": "Ancillary expenses (pharmacy, lab, radiology) per skilled day. Formula: Total Ancillary Expenses / Skilled Days",
      "denominator_type": "skilled_days",
      "payer_scope": "skilled"
    },
    {
      "term": "Dietary Cost PPD",
      "abbreviation": "snf_dietary_cost_ppd",
      "definition": "Dietary expenses per patient day. Formula: Total Dietary Expenses / Resident Days",
      "denominator_type": "resident_days",
      "payer_scope": "all"
    },
    {
      "term": "Administration Cost PPD",
      "abbreviation": "snf_admin_cost_ppd",
      "definition": "Administration expenses per patient day. Formula: Total Administration Expenses / Resident Days",
      "denominator_type": "resident_days",
      "payer_scope": "all"
    },
    {
      "term": "Contract Labor % (Nursing)",
      "abbreviation": "snf_contract_labor_pct_nursing",
      "definition": "Percentage of nursing labor costs from agency/contract staff. Formula: (Nursing Agency/Contract Cost / Total Nursing Expenses) × 100",
      "denominator_type": "resident_days",
      "payer_scope": "all"
    },
    {
      "term": "Operating Margin %",
      "abbreviation": "snf_operating_margin_pct",
      "definition": "Operating income as percentage of total revenue. Formula: ((Total Revenue - Total Operating Expenses) / Total Revenue) × 100",
      "denominator_type": "resident_days",
      "payer_scope": "all"
    },
    {
      "term": "Skilled Margin %",
      "abbreviation": "snf_skilled_margin_pct",
      "definition": "Margin on skilled payer revenue after therapy and ancillary costs. Formula: ((Skilled Revenue - Therapy Cost - Ancillary Cost) / Skilled Revenue) × 100",
      "denominator_type": "skilled_days",
      "payer_scope": "skilled"
    },
    {
      "term": "Occupancy % (Units)",
      "abbreviation": "sl_occupancy_pct_units",
      "definition": "Percentage of available units occupied. Formula: (Occupied Units / Available Units) × 100",
      "denominator_type": "occupied_units",
      "payer_scope": "all"
    }
  ]
